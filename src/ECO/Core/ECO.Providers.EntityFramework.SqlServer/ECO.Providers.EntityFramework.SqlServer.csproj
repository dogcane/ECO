<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFramework>net9.0</TargetFramework>
		<Nullable>enable</Nullable>
		<AssemblyTitle>ECO.Providers.EntityFramework.SqlServer</AssemblyTitle>
		<Product>Enterprise CLR Objects - Entity Framework SqlServer provider</Product>
		<Description>ECO.Providers.EntityFramework.SqlServer is the specific package for EFCore - Sql Server and its persistence unit implementation. It requires the base ECO.Providers.EntityFramework package.</Description>
		<Authors>Andrea Canegrati</Authors>
		<Copyright>Andrea Canegrati Â© 2007</Copyright>
		<PackageProjectUrl>https://github.com/dogcane/ECO</PackageProjectUrl>
		<IncludeSymbols>true</IncludeSymbols>
		<SymbolPackageFormat>snupkg</SymbolPackageFormat>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PackageIconUrl>icon.png</PackageIconUrl>
		<PackageId>ECO.Core.Providers.EntityFramework.SqlServer</PackageId>
		<PackageTags>ddd aggregate entity repository persistence efcore sqlserver</PackageTags>
	</PropertyGroup>
	<ItemGroup>
		<PackageReference Include="Microsoft.EntityFrameworkCore.SqlServer" Version="9.0.2" />
	</ItemGroup>
	<ItemGroup>
		<ProjectReference Include="..\ECO.Providers.EntityFramework\ECO.Providers.EntityFramework.csproj" />
	</ItemGroup>
	<ItemGroup>
		<None Include="_images\icon.png" Pack="true" PackagePath="\" />
	</ItemGroup>

</Project>
